      "@@@SET_BUILD_PROPERTY@gsutil_urls@{\"upload VM images|upload amd64 image|gsutil upload\": \"gs://cros-containers-staging/baguette/images/baguette_rootfs_amd64_2012-05-14-125321_.img.zstd\"}@@@"
    ]
  },
  {
    "cmd": [],
    "name": "upload VM images.upload arm64 image",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LINK@image@https://storage.googleapis.com/cros-containers-staging/baguette/images/baguette_rootfs_arm64_2012-05-14-125321_.img.zstd@@@"
    ]
  },
  {
    "cmd": [
      "python3",
      "-u",
      "RECIPE_MODULE[depot_tools::gsutil]/resources/gsutil_smart_retry.py",
      "--",
      "RECIPE_REPO[depot_tools]/gsutil.py",
      "----",
      "cp",
      "docker_export/baguette_rootfs_arm64.img.zstd",
      "gs://cros-containers-staging/baguette/images/baguette_rootfs_arm64_2012-05-14-125321_.img.zstd"
    ],
    "cwd": "[CACHE]/platform2/vm_tools/baguette_image",
    "infra_step": true,
    "name": "upload VM images.upload arm64 image.gsutil upload",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@2@@@",
      "@@@STEP_LINK@gsutil.upload@https://storage.cloud.google.com/cros-containers-staging/baguette/images/baguette_rootfs_arm64_2012-05-14-125321_.img.zstd@@@",
      "@@@SET_BUILD_PROPERTY@gsutil_urls@{\"upload VM images|upload amd64 image|gsutil upload\": \"gs://cros-containers-staging/baguette/images/baguette_rootfs_amd64_2012-05-14-125321_.img.zstd\", \"upload VM images|upload arm64 image|gsutil upload\": \"gs://cros-containers-staging/baguette/images/baguette_rootfs_arm64_2012-05-14-125321_.img.zstd\"}@@@"
    ]
  },
  {
    "name": "$result"
  }
