      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--patch_ref",
      "https://chromium.googlesource.com/crosvm/crosvm@refs/heads/main:refs/changes/56/123456/7",
      "--revision",
      "src@refs/heads/main",
      "--refs",
      "refs/heads/main"
    ],
    "cwd": "[CACHE]/builder",
    "env": {
      "DEPOT_TOOLS_REPORT_BUILD": "crosvm/try/builder/8945511751514863184",
